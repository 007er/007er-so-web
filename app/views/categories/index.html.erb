<div class="col-md-12">
  <table class="table table-hover">
    <thead>
      <tr>
        <td>ID</td>
        <td>Name</td>
        <td>CreateTime</td>
      </tr>
    </thead>
    <tbody>
      <% @categories.each do |category| %>
        <tr>
          <td><%= category.id %></td>
          <td><%= link_to(category.name, category_path(category)) %></td>
          <td><%= category.created_at.to_s %></td>

        </tr>
      <% end %>
    </tbody>
  </table>
</div>
